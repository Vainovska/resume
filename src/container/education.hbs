<div class="ms-5">
    {{#with main}}
    <main>
        <section class="card p-3 mb-3">
            <div class="card-body">
                <div class="card-title">
                    {{> title data="My education" big=true}}
                </div>
                <ul>
                    {{#each educations}}
                    <li>{{name}}
                        {{#unless isEnd}}(now study){{/unless}}
                    </li>
                    {{/each}}
                </ul>
            </div>
        </section>
        <section class="card p-3 mt-3">
            <div class="card-body">
                <div class="card-title">
                    {{> title data="My certificates" big=false}}
                </div>
                <ul>
                    {{#each certificates}}
                    <li>{{name}} ({{id}})</li>
                    {{/each}}
                </ul>
            </div>
        </section>
    </main>
    {{/with}}
</div>